@model MvcCV.Models.Entity.TblSertifikalarım
@{
    ViewBag.Title = "SertifikaEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h4>Sertifika Ekleme Sayfası</h4>
<br />
<form class="form-group" method="post">

    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />


    @Html.Label("Açıklama")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />

    <button class="btn btn-info">Ekle</button>


</form>


